[api_keys]
# OpenRouter API Key - 請到 https://openrouter.ai/ 註冊並取得 API Key
openrouter_api_key = your_openrouter_api_key_here

# Perplexity API Key - 請到 https://www.perplexity.ai/ 註冊並取得 API Key
PERPLEXITY_API_KEY = your_perplexity_api_key_here

# Google Gemini API Key (如果需要使用 Gemini 模型)
gemini_api_key = your_gemini_api_key_here

[settings]
# HTTP 請求超時時間（秒）
http_timeout = 60
